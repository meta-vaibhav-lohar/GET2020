<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>EAD-2</display-name>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>server.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>signup</servlet-name>
    <servlet-class>server.UserSignup</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>vehicle-register</servlet-name>
    <servlet-class>server.VehicleRegister</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>server.Home</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>server.Logout</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>colleague</servlet-name>
    <servlet-class>server.Colleague</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>updateUser</servlet-name>
    <servlet-class>server.UpdateUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>processUpdateUser</servlet-name>
    <servlet-class>server.ProcessUpdateUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>updateVehicle</servlet-name>
    <servlet-class>server.UpdateVehicle</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>processUpdateVehicle</servlet-name>
    <servlet-class>server.ProcessUpdateVehicle</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>vehicle-register</servlet-name>
    <url-pattern>/vehicleRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>colleague</servlet-name>
    <url-pattern>/colleague</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>updateUser</servlet-name>
    <url-pattern>/updateUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>processUpdateUser</servlet-name>
    <url-pattern>/processUpdateUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>updateVehicle</servlet-name>
    <url-pattern>/updateVehicle</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>processUpdateVehicle</servlet-name>
    <url-pattern>/processUpdateVehicle</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>home</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>